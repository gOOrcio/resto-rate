{
	"name": "resto-rate",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,js,jsx,svelte,json,md}\"",
		"check-types": "turbo run check-types"
	},
	"devDependencies": {
		"eslint": "^9.28.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"turbo": "^2.5.4",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "bun@1.1.18",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"dependencies": {
		"dotenv": "^16.5.0"
	}
