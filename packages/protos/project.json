{
	"name": "protos",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "packages/protos",
	"tags": [],
	"targets": {
		"generate": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "packages/protos",
				"command": "find src -name '*.proto' -exec protoc --go_out=generated/go --go-grpc_out=generated/go --proto_path=src {} +"
			}
		}
	}
}
