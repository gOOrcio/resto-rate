{
	"name": "protos",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "packages/protos",
	"tags": [],
	"targets": {
		"generate": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": ".",
				"commands": [
					"mkdir -p apps/go-app/src/generated",
					"find packages/protos/src -name '*.proto' -exec protoc --go_out=apps/go-app/src/generated --go-grpc_out=apps/go-app/src/generated --proto_path=. {} +"
				],
				"parallel": false
			}
		}
	}
}
