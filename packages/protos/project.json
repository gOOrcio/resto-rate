{
	"name": "protos",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "packages/protos",
	"tags": [],
	"targets": {
		"generate": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "packages/protos",
				"command": "sh -c 'protoc --proto_path=. --proto_path=$(go env GOPATH)/pkg/mod --go_out=../../ --gorm_out=../../ --go-grpc_out=../../ *.proto'"
			}
		},
		"test": {
			"executor": "@nx-go/nx-go:test"
		},
		"lint": {
			"executor": "@nx-go/nx-go:lint"
		}
	}
}
